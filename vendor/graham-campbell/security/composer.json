{
    "name": "graham-campbell/security",
    "description": "Security Is A Port Of The Security Class From Codeigniter 2.1 For Laravel 4.1",
    "keywords": ["laravel", "framework", "security", "codeigniter", "Security", "Laravel Security", "Laravel-Security", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/event-dispatcher": "2.4.*",
        "symfony/filesystem": "2.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.3.*"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Security\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Security\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
